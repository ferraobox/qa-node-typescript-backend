{
  "name": "qa-node-typescript-backend",
  "version": "0.2.0",
  "description": "REST Api testing",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ferraobox/qanodebackend.git"
  },
  "scripts": {
    "test": ". ./setup/runDocker.sh && jest src/test --forceExit && . ./setup/stopDocker.sh",
    "test:unit": "jest src/test/unit --forceExit",
    "test:int": ". ./setup/runDocker.sh && jest src/test/int --forceExit && . ./setup/stopDocker.sh",
    "test:contract": ". ./setup/runDocker.sh && jest src/test/contract --forceExit && . ./setup/stopDocker.sh",
    "test:debug": "jest src/test/contract/user.contract.test.ts --detectOpenHandles --watch --verbose=false --silent=false",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}'",
    "bump": "npm version $VERSION --no-git-tag-version",
    "postbump": "git add package.json package-lock.json && git commit -m 'Bump version'"
  },
  "jest": {
    "testMatch": [
      "src/test/*.test.ts",
      "src/test/**/*.test.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [
    "api",
    "testing"
  ],
  "author": "Carlos Ferrao",
  "license": "ISC",
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@types/faker": "^5.1.6",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "ajv": "^6.12.6",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "faker": "^5.3.1",
    "jest": "^26.6.3",
    "jest-html-reporters": "^2.1.2",
    "openapi-types": "^9.0.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "got": "^11.8.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.32"
  }
}
